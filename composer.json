    "extra": {
        "laravel": {
            "providers": [
                "ezavalishin\\SkablConnect\\SkablConnectServiceProvider"
            ],
            "aliases": {
                "SkablConnect": "ezavalishin\\SkablConnect\\Facades\\SkablConnect"
            }
        }
    }
